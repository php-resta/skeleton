        "zendframework/zend-crypt": "^3.3",
        "giggsey/libphonenumber-for-php": "^8.0@dev",
        "ext-pdo": "*",
      "ext-fileinfo": "*",
        "twilio/sdk": "dev-master",
        "pusher/pusher-php-server": "^3.4@dev"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "nunomaduro/collision": "^4.0"
    }
